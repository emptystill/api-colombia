pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/emptystill/api-colombia.git'
            }
        }

        stage('Build') {
            steps {
                dir('api-colombia') {
                    sh 'dotnet restore'
                    sh 'dotnet build'
                }
            }
        }

        stage('SonarQube Analysis') {
            steps {
                withSonarQubeEnv('SonarQube') {
                    sh '''
                    dotnet tool install --global dotnet-sonarscanner
                    dotnet sonarscanner begin /k:"tu_proyecto_key" /d:sonar.host.url="http://localhost:9000" /d:sonar.login="tu_token_de_autenticacion"

                    dotnet build
                    
                    dotnet sonarscanner end /d:sonar.login="tu_token_de_autenticacion"
                    '''
                }pipeline {
